<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <meta charset="utf-8"/>
    <link rel='stylesheet' href='/stylesheets/bloglist.css' />
    <link rel="stylesheet" href="/stylesheets/common/bootstrap.min.css">
</head>
<body>
<% for(var i=0; i < res.length; i++) { %>
    <div class="header"><%= res[i].title %></div>
<% } %>
<div class="content">
    <div class="bloglist">
        <% for(var i=0; i < res.length; i++) { %>
            <div id="showdown-text"><%- res[i].content %></div>
        <% } %>
    </div>
    <div id="artBottom">
        <!-- 多说评论框 start -->
        <% for(var i=0; i < res.length; i++) { %>
            <div class="ds-thread" data-thread-key="<%= res[i].id %>" data-title="<%= res[i].title %>" data-url="http://western-ranger.com/blog?id=<%= res[i].id %>"></div>
        <% } %>
        <!-- 多说评论框 end -->
    </div>
</div>

<% include footer %>
<!-- 多说公共JS代码 start (一个网页只需插入一次) -->
<script type="text/javascript">
var duoshuoQuery = {short_name:"western-ranger"};
    (function() {
        var ds = document.createElement('script');
        ds.type = 'text/javascript';ds.async = true;
        ds.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') + '//static.duoshuo.com/embed.js';
        ds.charset = 'UTF-8';
        (document.getElementsByTagName('head')[0] 
         || document.getElementsByTagName('body')[0]).appendChild(ds);
    })();
</script>
<!-- 多说公共JS代码 end -->
</body>
</html>
